{
  "name": "tgkw-adc/huaweicloud-obs-client",
  "description": "HuaweiCloud OBS Client for PHP",
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.0",
    "spatie/array-to-xml": "^3.3",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "TgkwAdc\\Obs\\": "src/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --config=.php-cs-fixer.php",
    "cs-check": "php-cs-fixer fix --config=.php-cs-fixer.php --dry-run --diff"
  },
  "authors": [
    {
      "name": "tgkw-adc"
    }
  ]
}
